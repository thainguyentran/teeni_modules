<odoo>
    <data>
        <record id="sequence_payment_voucher" model="ir.sequence">
            <field name="name">Payments Voucher</field>
            <field name="code">account.payment.voucher</field>
            <field name="prefix">V%(year)s/%(month)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>
    </data>
</odoo>
